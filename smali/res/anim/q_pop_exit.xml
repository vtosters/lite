        android:duration="167"
        android:fillAfter="true"
        android:fillBefore="true"
        android:fillEnabled="true"
        android:fromAlpha="1.0"
        android:interpolator="@anim/linear"
        android:startOffset="33"
        android:toAlpha="0.0" />
